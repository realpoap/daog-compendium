{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "api#build": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "web#build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    }
  }
}
